{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "index.html"
		},
        {
			"name": "Launch Noosfero NG",
			"type": "chrome",
			"request": "launch",
            "sourceMaps": true,
			"webRoot": "src/app",
			"url": "http://localhost:3001/"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost/mypage.html",
			"sourceMaps": true,
			"webRoot": "wwwroot"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
            "sourceMaps": true,
			"webRoot": "./",
			"port": 9222
		}
	]
}