{
  "name": "angular-theme",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --deploy=/assets/designs/themes/$npm_package_config_theme/ --prod --aot=false -op=dist/$npm_package_config_theme",
    "build-all": "for d in ./themes/*/ ; do (npm config set angular-theme:theme `basename $d` && npm run build); done",
    "test": "ng test --browser=Chrome",
    "test:ci": "ng test --browser=PhantomJS --code-coverage=true --single-run=true",
    "lint": "ng lint",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "theme:setup": "ln -sfn ../themes/$npm_package_config_theme src/theme",
    "prestart": "npm run theme:setup",
    "pretest": "npm run theme:setup",
    "pretest:ci": "npm run theme:setup",
    "prebuild": "npm run theme:setup"
  },
  "config": {
    "theme": "angular-default"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^7.0.0",
    "@toverux/ngsweetalert2": "^1.2.2",
    "@types/lodash": "^4.14.68",
    "angular-2-local-storage": "^1.0.1",
    "angular-tag-cloud-module": "^1.3.1",
    "angular2-moment": "^1.3.3",
    "bootstrap-sass": "^3.3.7",
    "ckeditor": "^4.7.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "mydatepicker": "^2.0.13",
    "ng-dynamic": "^3.0.2",
    "ng2-animate": "^1.2.1",
    "ng2-ckeditor": "^1.1.6",
    "ng2-dragula": "^1.3.0",
    "ng2-img-cropper": "^0.8.6",
    "ng2-page-scroll": "^4.0.0-beta.9",
    "ngx-bootstrap": "^1.7.1",
    "ngx-pipes": "1.4.5",
    "ngx-progressbar": "^2.0.3",
    "ngx-restangular": "^1.0.11",
    "ngx-toastr": "^5.2.5",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "~3.0.1",
    "cucumber": "^2.3.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.1.2",
    "protractor-cucumber-framework": "^3.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  },
