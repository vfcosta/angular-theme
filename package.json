{
  "name": "angular-theme",
  "version": "0.0.0",
  "dependencies": {
    "@angular/common": "2.4.5",
    "@angular/compiler": "2.4.5",
    "@angular/core": "2.4.5",
    "@angular/forms": "2.4.5",
    "@angular/http": "2.4.5",
    "@angular/platform-browser": "2.4.5",
    "@angular/platform-browser-dynamic": "2.4.5",
    "@angular/upgrade": "2.4.5",
    "angular": "^1.5.0",
    "angular-mock": "^1.0.0",
    "angular2-moment": "^1.1.0",
    "moment": "^2.17.1",
    "ng-dynamic": "^2.0.0",
    "ng-forward": "0.0.1-alpha.12",
    "ng2-bootstrap": "^1.3.3",
    "ng2-ckeditor": "^1.1.6",
    "ng2-img-cropper": "^0.8.0",
    "ngx-pipes": "^1.4.5",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  },
  "config": {
    "theme": "angular-default",
    "skin": "skin-whbl"
  },
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "webpack; gulp clean && gulp --theme=$npm_package_config_theme --skin=$npm_package_config_skin build",
    "build-all": "gulp clean && for d in ./themes/*/ ; do (webpack && gulp --theme=`basename $d` build); done",
    "webpack": "webpack",
    "karma": "concurrently \"webpack -w\" \"karma start\"",
    "docs": "gulp ngdocs; static-server docs",
    "coverage": "karma start --single-run",
    "test-single": "webpack && karma start --single-run",
    "test": "webpack -w --env.test",
    "jenkins": "webpack && karma start --single-run",
    "postinstall": "bower install --allow-root; typings install; cd dev-scripts; typings install; cd ../; npm run fix-ngforward",
    "start": "concurrently \"webpack -w\" \"gulp --theme=$npm_package_config_theme --skin=$npm_package_config_skin serve\"",
    "generate-indexes": "ts-node --project ./dev-scripts ./dev-scripts/generate-index-modules.ts",
    "fix-ngforward": "ts-node --project ./dev-scripts dev-scripts/fix-ngforward.ts",
    "debug-gulp": "webpack; gulp clean; ./node_modules/.bin/iron-node node_modules/gulp/bin/gulp.js --theme=$npm_package_config_theme --skin=$npm_package_config_skin"
  },
  "devDependencies": {
    "@types/angular": "^1.6.2",
    "@types/angular-animate": "^1.5.6",
    "@types/angular-cookies": "^1.4.3",
    "@types/angular-mocks": "^1.5.8",
    "@types/angular-resource": "^1.5.8",
    "@types/angular-route": "^1.3.3",
    "@types/angular-sanitize": "^1.3.4",
    "@types/jasmine": "^2.5.41",
    "@types/moment": "^2.13.0",
    "awesome-typescript-loader": "^3.0.8",
    "bower": "^1.7.7",
    "browser-sync": "~2.18.6",
    "browser-sync-spa": "~1.0.3",
    "chalk": "~1.1.1",
    "concurrently": "^2.0.0",
    "core-js": "^2.1.3",
    "del": "~2.0.2",
    "eslint-plugin-angular": "~0.12.0",
    "estraverse": "~4.1.0",
    "expose-loader": "^0.7.1",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-angular-filesort": "~1.1.1",
    "gulp-angular-templatecache": "~1.8.0",
    "gulp-autoprefixer": "~3.0.2",
    "gulp-eslint": "~1.0.0",
    "gulp-filter": "~3.0.1",
    "gulp-flatten": "~0.2.0",
    "gulp-import-css": "^0.1.2",
    "gulp-inject": "~3.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-load-plugins": "~0.10.0",
    "gulp-merge-json": "^0.4.0",
    "gulp-minify-css": "~1.2.1",
    "gulp-minify-html": "~1.0.4",
    "gulp-ng-annotate": "~1.1.0",
    "gulp-ngdocs": "github:nikhilmodak/gulp-ngdocs.git",
    "gulp-protractor": "~1.0.0",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "~0.5.4",
    "gulp-rev": "~6.0.1",
    "gulp-rev-replace": "~0.4.2",
    "gulp-sass": "~2.0.4",
    "gulp-size": "~2.0.0",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-typescript": "^3.1.5",
    "gulp-uglify": "~1.4.1",
    "gulp-useref": "~1.3.0",
    "gulp-util": "~3.0.6",
    "html-loader": "^0.4.4",
    "http-proxy-middleware": "~0.9.0",
    "iron-node": "^3.0.7",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "~1.4.0",
    "karma-angular-filesort": "~1.0.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "~0.3.6",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.8.1",
    "lodash": "~3.10.1",
    "main-bower-files": "~2.9.0",
    "map-stream": "0.0.6",
    "merge-stream": "^1.0.0",
    "node-sass": "^3.13.0",
    "on-build-webpack": "^0.1.0",
    "phantomjs": "~1.9.18",
    "phantomjs-polyfill": "0.0.2",
    "reflect-metadata": "^0.1.3",
    "remap-istanbul": "~0.8.4",
    "replace": "^0.3.0",
    "static-server": "^2.0.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.7.0",
    "tslint": "^3.5.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^2.1.5",
    "typings": "^2.0.0",
    "uglify-save-license": "~0.4.1",
    "vinyl-transform": "^1.0.0",
    "watchpack": "~1.2.1",
    "webpack": "^2.2.1",
    "wiredep": "~2.2.2",
    "wrench": "~1.5.8",
    "yargs": "^4.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
